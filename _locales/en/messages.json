  "usageGuideTitle": {
    "message": "How to Use ContextClipCalendar",
    "description": "Usage guide title"
  },
  "usageGuideStep1": {
    "message": "<strong>Copy Text:</strong> Copy the text you want to register as a schedule to the clipboard",
    "description": "Usage guide step 1"
  },
  "usageGuideStep2": {
    "message": "<strong>Open Extension:</strong> Click the ContextClipCalendar icon in the browser toolbar",
    "description": "Usage guide step 2"
  },
  "usageGuideStep3": {
    "message": "<strong>Auto Load:</strong> The text from the clipboard will be automatically loaded",
    "description": "Usage guide step 3"
  },
  "usageGuideStep4": {
    "message": "<strong>Register Schedule:</strong> Click the \"Add to Calendar\" button to register it to Google Calendar",
    "description": "Usage guide step 4"
  },
  "usageGuideStep5": {
    "message": "<strong>Context Menu:</strong> You can also use it by selecting text on a webpage and right-clicking",
    "description": "Usage guide step 5"
  },
  "aiFeaturesTitle": {
    "message": "What AI Automatically Processes",
    "description": "AI features title"
  },
  "aiFeature1": {
    "message": "<strong>Title Extraction:</strong> Automatically generates an appropriate event title from the text",
    "description": "AI feature 1"
  },
  "aiFeature2": {
    "message": "<strong>Date/Time Analysis:</strong> Recognizes natural language time expressions like \"tomorrow at 3 PM\", \"next Monday\", etc.",
    "description": "AI feature 2"
  },
  "aiFeature3": {
    "message": "<strong>Location Information:</strong> Extracts location information such as meeting rooms, addresses, online platforms, etc.",
    "description": "AI feature 3"
  },
  "aiFeature4": {
    "message": "<strong>Attendee Information:</strong> Creates a list of attendees based on email addresses",
    "description": "AI feature 4"
  },
  "aiFeature5": {
    "message": "<strong>Content Summary:</strong> Summarizes long text appropriately for event descriptions",
    "description": "AI feature 5"
  }
